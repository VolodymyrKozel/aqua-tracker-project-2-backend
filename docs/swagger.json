            "name": "Authorization",
            "in": "header",
            "description": "access token",
            "required": true,
            "schema": {
              "type": "string",
              "example": "eyJ1c2VybmFtZSI6ImFkbWluIiwicm9sZSI6ImFkbWluIiwiaWF0IjoxNjYyMTY0ODU4LCJleHAiOjE2NjIyNjQ4NTg4fQ"
            }
          },
          {
            "name": "id",
            "in": "query",
            "description": "UserId",
            "required": true,
            "schema": {
              "type": "string",
              "example": "66996a0e1a65dd13c4186b31"
